{
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:react/recommended"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"allowImportExportEverywhere": true,
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"rules": {
		"no-unused-vars": 2
	}
}
